<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.airtnt.airtnt.service.HostMapper">
	
	<select id="listGuide" resultType="GuideDTO">
		select * from guide
	</select>
	<select id="getGuide" parameterType="int" resultType="guideDTO">
		select * from guide where id = #{id}
	</select>
	<insert id="insertProperty" parameterType="propertyDTO">
		insert into property values(property_seq.nextval, #{name}, #{hostId}, 
		#{propertyDesc}, #{address}, #{latitude}, #{longitude}, #{price}, #{bedCount}, 
		#{maxGuest})<!-- modeDate==null -->
	</insert>
	<delete id="deleteProperty" parameterType="int">
		
	</delete>
	<select id="propertyType" resultType="propertyTypeDTO">
		select * from property_type order by id
	</select>
	<select id="subPropertyType" parameterType="int" resultType="subPropertyTypeDTO">
		select * from sub_property_type where property_type_id = #{propertyTypeId}
	</select>
	<select id="roomType" resultType="roomTypeDTO">
		select * from room_type
	</select>
